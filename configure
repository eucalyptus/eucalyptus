        { { $as_echo "$as_me:$LINENO: error: Cannot find jni.h in $JAVA_HOME: do you have a JDK installed?" >&5
$as_echo "$as_me: error: Cannot find jni.h in $JAVA_HOME: do you have a JDK installed?" >&2;}
   { (exit 1); exit 1; }; }
fi

ant_version=`$ANT -version 2>&1 | grep "Ant.* version" | \
        sed -e 's/.*Ant.* version \([0-9.]*\).*/\1/'`
goodversion=`expr $ant_version ">=" $ant_min_version`
if test $goodversion -eq 0; then
        { { $as_echo "$as_me:$LINENO: error: Eucalyptus needs at least ANT version $ant_min_version" >&5
$as_echo "$as_me: error: Eucalyptus needs at least ANT version $ant_min_version" >&2;}
   { (exit 1); exit 1; }; }
fi
# some version of ant picks up the wrong java
java_version=`$ANT -diagnostics 2>&1 | grep ^java.version | \
        sed -e 's/java.* \([0-9.]*\).*/\1/'`
goodversion=`expr $java_version ">=" $java_min_version`
if test $goodversion -eq 0; then
        { { $as_echo "$as_me:$LINENO: error: ANT is using the wrong java (version less than $java_min_version)" >&5
$as_echo "$as_me: error: ANT is using the wrong java (version less than $java_min_version)" >&2;}
   { (exit 1); exit 1; }; }

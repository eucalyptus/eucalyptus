java_version=`$JAVA -version 2>&1 | grep "java version" | \
        sed -e 's/.*java version "\(.*\)".*/\1/'`
goodversion=`expr $java_version ">=" $java_min_version`
if test $goodversion -eq 0; then
        { { $as_echo "$as_me:$LINENO: error: Eucalyptus needs at least JDK version $java_min_version" >&5
$as_echo "$as_me: error: Eucalyptus needs at least JDK version $java_min_version" >&2;}
   { (exit 1); exit 1; }; }
fi

# now let's test we have jni.h
